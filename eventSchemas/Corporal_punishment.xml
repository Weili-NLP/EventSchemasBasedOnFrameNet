    </Arg>
    <Arg bgColor="9F79EE" fgColor="FFFFFF" coreType="Peripheral" cBy="MJE" cDate="08/30/2001 06:00:58 PDT Thu" abbrev="BodP" name="Body_part" ID="1802">
        <definition>&lt;def-root&gt;This FE is the location on the body where the bodily injury takes place.&lt;/def-root&gt;</definition>
        <semType name="Body_part" ID="10"/>
    </Arg>
    <Arg bgColor="9400D3" fgColor="FFFFFF" coreType="Extra-Thematic" cBy="MJE" cDate="08/30/2001 06:01:08 PDT Thu" abbrev="Depict" name="Depictive" ID="1803">
        <definition>&lt;def-root&gt;Depictive phrase describing the actor of an action&lt;/def-root&gt;</definition>
    </Arg>
    <Arg bgColor="FF69B4" fgColor="FFFFFF" coreType="Peripheral" cBy="MJE" cDate="08/30/2001 06:06:10 PDT Thu" abbrev="Degr" name="Degree" ID="1804">
